    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "tslint": "^5.20.1"
  },
  "scripts": {
    "start": "rescripts start",
    "start:dev": "NODE_ENV=development yarn install && yarn start",
    "start:prod": "NODE_ENV=production yarn clean && yarn build && node server/app.js",
    "build": "rescripts build",
    "test:lint:js": "eslint src/ --ext=js --ext=jsx --ext=ts --ext=tsx --fix",
    "test:lint:styled": "stylelint --config=.stylelintrc '**/*.styled.*'",
    "test:lint": "run-s test:lint:**",
    "test:unit": "rescripts test --passWithNoTests",
    "test": "run-s test:**",
    "clean": "rm -rf build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint src/ --ext=js --ext=jsx --ext=ts --ext=tsx --fix",
      "prettier --config .prettierrc --write",
      "git add ."
    ],
    "src/**/*.styled.*": [
      "stylelint --config=.stylelintrc --fix",
      "prettier --config .prettierrc --write",
      "git add ."
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "rescripts": [
    "env"
  ]
}
